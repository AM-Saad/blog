<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <%- include('./includes/styles.ejs') %>
        <link rel="stylesheet" href="/admin/css/dashboardnew.css" />

</head>

<body>
    <%- include('./includes/side-nav.ejs') %>

        <div class="wrapper">
            <main>
                <section class="overall-info ">
                
                    <div class="gradient-green overall-info_item">
                        <p>subscribers</p>
                        <b>
                            <%=subscriperlength%>
                        </b>
                    </div>
                    <div class="gradient-yellow overall-info_item">
                        <p>Articles</p>
                        <b>
                            <%=totalArticles%>
                        </b>
                    </div>
                </section>
                <aside class="grid g-two ">
                    <div>
                        <div class="new-sales p-3  border-r-l  ">
                            <div class="flex f-space-between p-medium border-r-m ">
                                <h3>New Articles</h3>
                                <a href="/articles/" class="btn border-r-l bg-w">See More</a>
                            </div>
                            <% if(articles.length> 0) { %>
                                <div>
                                    <% articles.forEach(a=>{ %>
                                        <a href="/admin/articles"
                                            class="flex f-space-between m-3 p-medium font-m border-r-l bg-w article"
                                            data-si="<%=a._id%>">
                                        <span><%=a.title%></span>
                                        </a>
                                        <% }) %>
                                </div>
                                <% } else { %>
                                    <h5 class="p-3 c-b"> No new articles yet..</h5>
                                    <% } %>
                        </div>
                        <div class="new-customers p-3  border-r-l">
                            <div class="flex f-space-between p-3">
                                <h3>New subscribers</h3>
                                <a href="/customers/" class="btn border-r-l bg-w">See More </a>
                            </div>
                            <% if(subscribers.length> 0) { %>
                                <div>
                                    <% subscribers.forEach(c=>{ %>
                                        <div class="flex f-space-between m-3  p-medium font-m border-r-l bg-w">
                                            <span>
                                                <%=c.name %>
                                            </span>
                                          
                                        </div>
                                        <% }) %>
                                </div>
                                <% } else { %>
                                    <h5 class="p-3 c-b">No new subscriper yet..</h5>
                                    <% } %>
                        </div>
                    </div>
                    <div class="over-all-review p-relative p-3  border-r-l ">
                        <div class="p-3 flex">
                            <h3>Overview</h3>
                        </div>
                        <img class="loading " src="/admin/images/loading(3).svg">

                        <div class="none">
                            <canvas id="compareTwoMonthessales"></canvas>
                        </div>
                        <div class="none">
                            <canvas id="compareTwoMonthessalesreveneu"></canvas>
                        </div>
                    </div>
                </aside>
            </main>

        </div>
        <%- include('./includes/scripts.ejs') %>

            <script src="/admin/modules/interface/home.js"> </script>
            <!-- <script src="/admin/modules/interface/orders.js"> </script> -->
</body>

</html>